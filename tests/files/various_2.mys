class AccessFoo:
    member: bool

    def func(self):
        pass

@test
def test_class_none_1():
    foo: AccessFoo = None
    res = 0

    assert foo is None

    try:
        print(foo.member)
    except NoneError:
        res = 1

    assert res == 1

    try:
        foo.func()
    except NoneError:
        res = 2

    assert res == 2

@test
def test_class_none_2():
    foo: AccessFoo = None
    foo2 = foo

    assert foo2 is foo

    foo = AccessFoo(True)

    assert foo is not foo2
    assert foo2 is None
    assert foo.member

@test
def test_float_literal():
