    assert res == 3

@test
def test_iterate_over_zip_not_same_lengths_4():
    res: [i64] = []

    for (i, j), k in zip(enumerate(range(2)), slice(range(4), 2)):
        res.append(i)
        res.append(j)
        res.append(k)

    assert res[0] == 0
    assert res[1] == 0
    assert res[2] == 2
    assert res[3] == 1
    assert res[4] == 1
    assert res[5] == 3

